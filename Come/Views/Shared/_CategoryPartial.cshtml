@model IEnumerable<Come.Models.Category>

@{
    ViewData["Title"] = "Index";
}



<ul class="list-group list-group-flush">
    @foreach (var item in Model)
    {
        <li class="list-group-item">      @Html.DisplayFor(modelItem => item.Image)  @Html.DisplayFor(modelItem => item.Name)</li>
    }
</ul>
